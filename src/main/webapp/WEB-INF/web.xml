<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
         http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

    <!-- Welcome Page -->
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

    <!-- Servlet Configurations -->
    <servlet>
        <servlet-name>AddUserServlet</servlet-name>
        <servlet-class>com.example.usermanagement.servlet.AddUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddRoomServlet</servlet-name>
        <servlet-class>com.example.usermanagement.servlet.AddRoomServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditUserServlet</servlet-name>
        <servlet-class>com.example.usermanagement.servlet.EditUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteUserServlet</servlet-name>
        <servlet-class>com.example.usermanagement.servlet.DeleteUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListUsersServlet</servlet-name>
        <servlet-class>com.example.usermanagement.servlet.ListUsersServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>EditRoomServlet</servlet-name>
        <servlet-class>com.example.usermanagement.servlet.EditRoomServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteRoomServlet</servlet-name>
        <servlet-class>com.example.usermanagement.servlet.DeleteRoomServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddReservationServlet</servlet-name>
        <servlet-class>com.example.usermanagement.servlet.AddReservationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListReservationsServlet</servlet-name>
        <servlet-class>com.example.usermanagement.servlet.ListReservationsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListRoomsServlet</servlet-name>
        <servlet-class>com.example.usermanagement.servlet.ListRoomsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditReservationServlet</servlet-name>
        <servlet-class>com.example.usermanagement.servlet.EditReservationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteReservationServlet</servlet-name>
        <servlet-class>com.example.usermanagement.servlet.DeleteReservationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DashboardServlet</servlet-name>
        <servlet-class>com.example.usermanagement.servlet.DashboardServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignUpServlet</servlet-name>
        <servlet-class>com.example.usermanagement.servlet.SignUpServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.example.usermanagement.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClientDashboardServlet</servlet-name>
        <servlet-class>com.example.usermanagement.servlet.client.ClientDashboardServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClientListAvailableRoomsServlet</servlet-name>
        <servlet-class>com.example.usermanagement.servlet.client.ClientListAvailableRoomsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClientMakeReservationServlet</servlet-name>
        <servlet-class>com.example.usermanagement.servlet.client.ClientMakeReservationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClientEditReservationServlet</servlet-name>
        <servlet-class>com.example.usermanagement.servlet.client.ClientEditReservationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClientCancelReservationServlet</servlet-name>
        <servlet-class>com.example.usermanagement.servlet.client.ClientCancelReservationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>com.example.usermanagement.servlet.LogoutServlet</servlet-class>
    </servlet>
    <!-- Servlet Mappings -->
    <servlet-mapping>
        <servlet-name>AddUserServlet</servlet-name>
        <url-pattern>/addUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditUserServlet</servlet-name>
        <url-pattern>/editUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteUserServlet</servlet-name>
        <url-pattern>/deleteUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListUsersServlet</servlet-name>
        <url-pattern>/listUsers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditRoomServlet</servlet-name>
        <url-pattern>/editRoom</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteRoomServlet</servlet-name>
        <url-pattern>/deleteRoom</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddReservationServlet</servlet-name>
        <url-pattern>/addReservation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListReservationsServlet</servlet-name>
        <url-pattern>/listReservations</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListRoomsServlet</servlet-name>
        <url-pattern>/listRooms</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddRoomServlet</servlet-name>
        <url-pattern>/addRoom</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>EditReservationServlet</servlet-name>
        <url-pattern>/editReservation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteReservationServlet</servlet-name>
        <url-pattern>/deleteReservation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DashboardServlet</servlet-name>
        <url-pattern>/dashboard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignUpServlet</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClientDashboardServlet</servlet-name>
        <url-pattern>/clientdashboard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClientListAvailableRoomsServlet</servlet-name>
        <url-pattern>/client/availableRooms</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClientMakeReservationServlet</servlet-name>
        <url-pattern>/client/makeReservation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClientEditReservationServlet</servlet-name>
        <url-pattern>/client/editReservation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClientCancelReservationServlet</servlet-name>
        <url-pattern>/client/cancelReservation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>

</web-app>
